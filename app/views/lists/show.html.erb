<p><strong><%= @list.name %></strong></p>

<ul class="list-group">
  <% @list.bookmarks.each do |bookmark, movie| %>
    <li class="list-group-item"><%= bookmark.movie.title %>    <%= bookmark.comment %> </li>
  <% end %>
</ul>

<%= link_to "New bookmark", new_list_bookmark_path(@list) %>
<%= link_to "Back to List", root_path %>
